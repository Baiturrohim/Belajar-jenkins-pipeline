pipeline {
    agent {
      node {
	label "linux && java11" 	
       }
      }        
    stages {
        stage("hello") {
            steps {
                echo "Hello Pipeline"
            }
        }
    }
	post {
	 always {
	    echo "I will always say Hello again!"
	}
	success {
	  echo "Alhamdulillah success"
	}
	failure {
	 echo "Oh no, failure"
	}
	cleanup {
	 echo "Don't care succes or error"
      }
    }
}
